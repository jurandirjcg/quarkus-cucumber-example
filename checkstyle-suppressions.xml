<?xml version="1.0"?>
 
<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">
 
<suppressions>
  <suppress files="/generated-sources/" checks="[a-zA-Z0-9]*"/>
  <suppress checks="MissingJavadocMethod" files="(/entity/|/rest/\w*/response/|/rest/\w*/request/)"/>
  <suppress checks="FinalParameters" files="(/entity/|/rest/\w*/response/|/rest/\w*/request/)"/>
  <suppress checks="DesignForExtension" files=".java"/>
  <suppress checks="NewlineAtEndOfFile" files=".java"/>
  <suppress checks="ParameterNumber" files=".java"/>
